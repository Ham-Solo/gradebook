<h1>Student List</h1>

<table>
  <thead>
    <tr>
      <th>Assignment</th>
      <th>Grade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student.assignments.each do |a| %>
      <tr>
        <td><%= a.name %></td>
        <td><%= a.grade %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'New Teacher', new_teacher_path %>
<%= link_to 'New Parent', new_parent_path %>
<%= link_to 'New Student', new_student_path %>
<%= link_to 'Home', root_path %>
